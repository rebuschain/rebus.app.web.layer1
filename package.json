{
	"name": "rebus.app",
	"version": "0.0.1",
	"description": "",
	"scripts": {
		"build": "yarn build:css && cross-env NODE_ENV=production webpack",
    "build:mainnet": "node build-vercel.js",
    "build:testnet": "node build-vercel-testnet.js",
		"serve": "http-server --address 0.0.0.0 --port 8080 --proxy http://localhost:8080? ./prod",
		"dev": "cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=development react-env && yarn build:css && webpack serve",
		"dev:alphanet": "yarn react-env:alphanet && cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=development webpack serve",
		"dev:testnet": "yarn react-env:testnet && cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=development webpack serve",
		"dev:testnet:local": "yarn react-env:testnet:local && cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=development webpack serve",
		"dev:mainnet": "yarn react-env:mainnet && cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=development webpack serve",
		"dev:mainnet:local": "yarn react-env:mainnet:local && cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=development webpack serve",
		"dev:analyzer": "cross-env NODE_ENV=development ANALYZER=true webpack serve",
		"test": "jest --passWithNoTests --runInBand",
		"lint-test": "eslint \"src/**/*\" webpack.config.js && prettier --check \"src/**/*\" webpack.config.js",
		"lint-fix": "eslint --fix \"src/**/*\" webpack.config.js && prettier --write \"src/**/*\" webpack.config.js",
		"build:css": "tailwindcss build src/styles/tailwind.scss -o src/styles/index.scss",
		"prettify": "prettier --write './**/*.{js,jsx,ts,tsx,scss,md,json,html}' --config ./.prettierrc",
		"react-env": "react-env --env APP_ENV",
		"react-env:alphanet": "cross-env APP_ENV=alphanet react-env --env APP_ENV",
		"react-env:testnet": "cross-env APP_ENV=testnet react-env --env APP_ENV",
		"react-env:testnet:local": "cross-env APP_ENV=testnet.local react-env --env APP_ENV",
		"react-env:mainnet": "cross-env APP_ENV=mainnet react-env --env APP_ENV",
		"react-env:mainnet:local": "cross-env APP_ENV=mainnet.local react-env --env APP_ENV",
		"postinstall": "patch-package",
		"proto": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=./src/proto --js_out=import_style=commonjs,binary:./src/proto -I=./proto ./proto/rebus/nftid/v1/*.proto ./proto/evmos/erc20/v1/*.proto ./proto/google/api/*.proto ./proto/gogoproto/*.proto ./proto/cosmos/base/v1beta1/*.proto",
		"proto:ts": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=esModuleInterop=true --ts_proto_out=./src/proto -I=./proto ./proto/rebus/nftid/v1/tx.proto ./proto/evmos/erc20/v1/tx.proto"
	},
	"pre-commit": [
		"lint-test"
	],
